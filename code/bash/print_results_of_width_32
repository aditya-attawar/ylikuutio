#!/bin/bash
for i in $(cat "$1" | xxd -p | tr -d '\n' | sed -E 's/(..)(..)/\2\1 /g'); do printf "%d " $((16#$i)); done | sed 's/\([0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* [0-9]* *\)/\1\n/g' | sed 's/[0 ]*$//g' | sed 's@\<9999\>@N/A@g' | column -t
